INCEPUTUL_DECLARATIILOR

INTREG a. REAL b. CARACTER sir. FIXAT INTREG R.
INTREG c().
REAL d(INTREG e ~ REAL f).
INTREG g[100].
STRUCTURA A
{
    INTREG a.
    STRUCTURA b
    {
        REAL c.
        INTREG u[100].
    }.
}.

SFARSITUL_DECLARATIILOR

START
{

DACA (ADEVARAT)
    ATUNCI a<-a. 
SFARSIT_DACA.
1+1*2.

PENTRU i<-1 PANA_LA i<10 EXECUTA_FOR
a<-10.
SFARSIT_FOR.

CAT_TIMP i > 6 EXECUTA_WHILE
a<-10.
SFARSIT_WHILE.

}
STOP

INCEPUTUL_DECLARATIILOR_FUNCTIILOR

INTREG a(INTREG b)
{
    a<-a.
    RETURNEAZA INTREG.
}
REAL d(INTREG e ~ REAL f)
{
    a<-a.
    RETURNEAZA REAL.
}

SFARSITUL_DECLARATIILOR_FUNCTIILOR